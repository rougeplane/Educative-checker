{
    "manifest_version": 3,
    "name": "Educative checker",
    "version": "0.1.0",
  
    "content_scripts": [
        {
            "matches": [
                "https://*.educative.io/*"
            ],
            "js": ["code.js"]
        }
    ],

    "background" : {
        "service_worker": "background.js"
    },

    "action": {
    },
    "permissions": [
        "storage",
        "unlimitedStorage"
    ]

}